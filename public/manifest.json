{
  "manifest_version": 2,
  "name": "PicPickDL",
  "version": "0.1.0",
  "description": "ページ内の画像をいい感じにダウンロード",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "*://*/*",
    "activeTab"
  ],
  "background": {
    "scripts": [ "browser-polyfill.js", "background.js" ],
    "persistent": false
  },
  "devtools_page": "devtools.html"
}